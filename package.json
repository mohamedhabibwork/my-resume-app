{
	"name": "my-resum-app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": "^20.19.0 || >=22.12.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "npm run build && wrangler dev",
		"build-only": "vite build",
		"type-check": "vue-tsc --build",
		"deploy": "npm run build && wrangler deploy",
		"cf-typegen": "wrangler types",
    "test": "npm run lint && npm run deploy",
		"lint": "eslint . --ext .vue,.ts,.tsx",
		"lint:fix": "eslint . --ext .vue,.ts,.tsx --fix",
		"format": "prettier --write \"src/**/*.{vue,ts,tsx,css}\"",
		"format:check": "prettier --check \"src/**/*.{vue,ts,tsx,css}\"",
		"check": "run-p type-check lint:fix format",
		"prepare": "husky"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.18",
		"@vueuse/core": "^14.1.0",
		"@vueuse/head": "^2.0.0",
		"firebase": "^12.8.0",
		"html2canvas": "^1.4.1",
		"html2pdf.js": "^0.14.0",
		"idb": "^8.0.3",
		"jspdf": "^4.0.0",
		"pinia": "^3.0.4",
		"tailwindcss": "^4.1.18",
		"vite-plugin-pwa": "^1.2.0",
		"vue": "^3.5.26",
		"vue-meta": "^2.4.0",
		"vue-router": "^4.6.4"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.21.0",
		"@eslint/js": "^9.18.0",
		"@tsconfig/node24": "^24.0.3",
		"@types/node": "^25.0.9",
		"@vitejs/plugin-vue": "^6.0.3",
		"@vue/tsconfig": "^0.8.1",
		"daisyui": "^5.5.14",
		"eslint": "^9.18.0",
		"eslint-plugin-vue": "^10.0.0",
		"globals": "^17.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.4.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^7.3.0",
		"vite-plugin-vue-devtools": "^8.0.5",
		"vue-eslint-parser": "^10.1.1",
		"vue-tsc": "^3.2.2",
		"wrangler": "^4.59.2"
	}
}
